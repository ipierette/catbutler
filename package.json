{
  "name": "catbutler-monorepo",
  "version": "1.0.0",
  "description": "CatButler - Assistente de IA para Casa (Frontend + Backend)",
  "private": true,
  "workspaces": [
    "catbutler-frontend",
    "catbutler-backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd catbutler-frontend && npm run dev",
    "dev:backend": "cd catbutler-backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd catbutler-frontend && npm run build",
    "build:backend": "cd catbutler-backend && npm run build",
    "install:all": "npm install && cd catbutler-frontend && npm install && cd ../catbutler-backend && npm install",
    "start": "npm run start:backend",
    "start:backend": "cd catbutler-backend && npm start",
    "start:frontend": "cd catbutler-frontend && npm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "catbutler",
    "ai",
    "assistant",
    "home",
    "monorepo",
    "react",
    "nodejs",
    "supabase",
    "huggingface"
  ],
  "author": "Izadora",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipierette/catbutler.git"
  }
}
